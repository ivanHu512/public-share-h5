{
  "name": "stardust-h5",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "node scripts/test.js",
    "build:test": "cross-env REACT_APP_ENV=test node scripts/build.js && npm run zip && git add dist.zip && git commit -m 'chore: build test'",
    "build:prod": "cross-env REACT_APP_ENV=prod node scripts/build.js && npm run zip && git add dist.zip",
    "build:gray": "cross-env REACT_APP_ENV=gray node scripts/build.js && npm run zip && git add dist.zip",
    "bt": "npm run build:test",
    "bp": "npm run build:prod",
    "bg": "npm run build:gray",
    "build:a": "cross-env REACT_APP_ENV=prod node scripts/build.js $*",
    "build-kiss:test": "cross-env PROJECT_NAME=kiss REACT_APP_ENV=test node scripts/build.js $*",
    "build-kiss:prod": "cross-env PROJECT_NAME=kiss REACT_APP_ENV=prod node scripts/build.js $*",
    "start": "cross-env node scripts/start.js $*",
    "dev": "npm run start",
    "start-kiss:test": "cross-env PROJECT_NAME=kiss REACT_APP_ENV=test node scripts/start.js $*",
    "start-kiss:prod": "cross-env PROJECT_NAME=kiss REACT_APP_ENV=prod node scripts/start.js $*",
    "start-kiss": "cross-env PROJECT_NAME=kiss node scripts/start.js $*",
    "dev:fb": "cross-env PROJECT_NAME=feedback REACT_APP_ENV=dev node scripts/start.js $*",
    "skt": "npm run start-kiss:test",
    "skp": "npm run start-kiss:prod",
    "bkt": "npm run build-kiss:test",
    "bkp": "npm run build-kiss:prod",
    "zip": "rm -f dist.zip && zip -q -r dist.zip ./dist"
  },
